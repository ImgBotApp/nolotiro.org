<div id="main">
  <div class="span-18">
    <div class="ad_excerpt"> 
      <div class="error">
      <% if defined? @q %>
        No hay anuncios que coincidan
        con la búsqueda <b><%= @q %></b>
        en la ubicación <%= link_to WoeidHelper.convert_woeid_name(@id), ads_woeid_path(@id, type: "give") %>.
      <% else %>
        Debes buscar por algo en algun lugar.
      <% end %>
      </div>
    </div>
  </div>
</div>
