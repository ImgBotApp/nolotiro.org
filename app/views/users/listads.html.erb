<% content_for :meta_title, "#{t('nlt.published_ads')} - #{@user.username}" %>

<div id="default_header_section">
  <h1><%= t 'nlt.published_ads' %>  -  <%= @user.username %></h1>
</div>

<div id="main">
  <div class="span-19">
    <%= render partial: "users/tabnav", locals: {user: @user} %>
    <% @ads.each do |ad| %>
      <%= render :partial => "ads/ad", :locals => { :ad => ad } %>
    <% end %>
    <%= render partial: "partials/pagination", locals: { items: @ads } %>
  </div>
  <%= render partial: "partials/google_adsense", :locals => {:class_name => "vertical_ad"}  %>
</div>
