<%= content_for(:meta_title, t('nlt.change_your_city')) %>

<div id="default_header_section">
  <h1><%= t('nlt.change_your_city') %></h1>
</div>

<div id="main">
  <div class="col-md-8">
    <%= render partial: "ask_form", :locals => { location_suggest: @location_suggest } %>
  </div>
  <div class="col-md-4">
    <%= render partial: "partials/google_adsense", locals: { data_format: "rectangle", class_name: "rectangular_ad desktop" } %>
    <%= render partial: "partials/google_adsense", locals: { data_format: "horizontal", class_name: "mobile" } %>
  </div>
</div>
